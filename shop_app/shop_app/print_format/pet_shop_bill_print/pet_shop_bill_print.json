 "html": "\n<style>\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tline-height: 75%;\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 4in;\n\t\t\tpadding: 0.25in;\n\t\t\tmin-height: 8in;\n\t\t}\n\t}\n</style>\n<table>\n\t<tbody>\n\t\t<tr>\n\t\t\t<!--<td style=\"width: 60%\"><b>{{ _(\"Bill No\") }}:</b> {{ doc.bill_id }}</td>-->\n\t\t\t<td class=\"text-center\" style=\"margin-bottom: 1rem\"><b>{{ ((\"SHIMNAS PET SHOP\")) }}</b></td>\n\t\t</tr>\n\t\t<tr>\n\t\t    <td class=\"text-center\" style=\"margin-bottom: 1rem\"><b>{{ ((\"Mangattukavala\")) }}</b></td>\n\t\t</tr>\n\t\t<tr>\n\t\t    <td class=\"text-center\" style=\"margin-bottom: 1rem\"><b>{{ (\"Customer Bill\") }}</b></td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table class=\"table no-border\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td style=\"width: 60%\"><b>{{ _(\"Bill No\") }}:</b> {{ doc.bill_id }}</td>\n\t\t\t<td class=\"text-right\" style=\"width: 60%\"><b>{{ _(\"GSTN\") }}:</b> {{ doc.gst }}</td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td style=\"width: 60%\"><b>{{ _(\"Date\") }}:</b> {{ doc.get_formatted(\"date_of_transaction\") }}</td>\n\t\t\t<td class=\"text-right\" style=\"width: 60%\"><b>{{ _(\"Contact\") }}:</b> {{ doc.phone_number }}</td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td style=\"width: 60%\"><b>{{ _(\"Customer Name\") }}:</b> {{ doc.customer_name }}</td>\n\t\t\t<!--<td class=\"text-right\" style=\"width: 60%\"><b>{{ _(\"License\") }}:</b> {{ doc.shop_owner }}</td>-->\n\t\t</tr>\n\t</tbody>\n</table>\n<p class=\"text-center\">{{ _(\"Cash sales\") }}</p>\n<table class=\"table table-condensed\">\n\t<thead>\n\t\t<tr>\n\t\t    <th width=\"40%\">{{ _(\"No\") }}</th>\n\t\t\t<th width=\"40%\">{{ _(\"PET name\") }}</th>\n\t\t\t<th width=\"40%\">{{ _(\"PET Category\") }}</th>\n\t\t\t\n\t\t\t<th width=\"25%\" class=\"text-right\">{{ _(\"Pet Price\") }}</th>\n\t\t\t<th width=\"25%\" class=\"text-right\">{{ _(\"QTY\") }}</th>\n\t\t</tr>\n\t</thead>\n\t<tbody>\n\t\t{%- for item in doc.ordered_items -%}\n\t\t<tr><td>{{ item.idx }}</td>\n\t\t\t<td class=\"text-right\">{{ item.pet_name }}</td>\n\t\t\t<td class=\"text-right\">{{ item.pet_category }}</td>\n\t\t\t\n\t\t\t<td class=\"text-right\">{{ item.pet_price }}</td>\n\t\t\t<td class=\"text-right\">{{ item.number }}</td>\n\t\t</tr>\n\t\t{%- endfor -%}\n\t</tbody>\n</table>\n<table class=\"table table-condensed no-border\">\n\t<tbody>\n\t\t<tr>\n\t\t    <td></td>\n\t\t\t<td class=\"text-right\" style=\"width: 60%\"><b>{{ _(\"Total\") }}:</b> {{ doc.total }}</td>\n\t\t</tr>\n        <tr>\n            <td style=\"width: 60%\"><b>{{ _(\"gst: 12%\") }}</b></td>\n            <td class=\"text-right\" style=\"width: 60%\"><b>{{ _(\"Discount\") }}:</b> {{ doc.discount }}</td>\n\t\t\t\n\t\t</tr>\n\t\t<tr>\n\t\t    <td></td>\n\t\t    <td class=\"text-right\" style=\"width: 60%\"><b>{{ _(\"Net Amount\") }}:</b><b>{{ doc.get_formatted(\"net_amount\", doc) }}</b></td>\n\t\t</tr>\n\t</tbody>\n</table>\n<!--<p class=\"text-right\"><b>{{ _(\"Total\") }}:</b> {{ doc.total }}<br></p>-->\n<!--<p class=\"text-right\"><b>{{ _(\"Discount\") }}:</b> {{ doc.discount_amount }}<br></p>-->\n<!--<p class=\"text-right\"><b>{{ _(\"Net amount\") }}:</b> {{ doc.net_amount }}<br></p>-->\n<p class=\"text-center\">{{ _(\"Please check the items before leaving the counter\") }}</p>\n<p class=\"text-center\">{{ _(\"Terms and conditions\") }}</p>\n<p class=\"text-center\">{{ _(\"Thank you, please visit again.\") }}</p>",
